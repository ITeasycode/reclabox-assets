body {
  margin: 0;
  padding: 0;
  background: #fff;
}

div {
  font-size: 11px;
  font-family: arial, sans-serif;
}

li {
  font-size: 11px;
  padding-bottom: 10px;
  list-style-type: none;
  font-family: arial, sans-serif;
}

ul {
  padding: 0px 10px;
  margin: 0;
}

img {
  border: 0;
}

a:link {
  color: #900;
  text-decoration: none;
  font-size: 12px;
  font-weight: bold;
}

a:visited {
  color: #900;
  text-decoration: none;
  font-size: 12px;
  font-weight: bold;
}

a:hover {
  text-decoration: underline;
  color: #900;
}

a:active {
  text-decoration: underline;
  color: #900;
}

a.sch:link {
  color: #fff;
  text-decoration: none;
  font-size: 12px;
  font-weight: bold;
  background: url('<%= asset_path('fbw.gif') %>');
  padding: 0 5px;
}

a.sch:visited {
  color: #fff;
  text-decoration: none;
  font-size: 12px;
  font-weight: bold;
  background: url('<%= asset_path('fbw.gif') %>');
  padding: 0 5px;
}

a.sch:hover {
  text-decoration: none;
  color: #fff;
  background: url('<%= asset_path('frot.gif') %>');
}

a.sch:active {
  text-decoration: none;
  color: #fff;
  background: url('<%= asset_path('frot.gif') %>');
}

#column {
  border: 1px solid #ccc;
  padding: 0 0 10px 0;
}

#logolink {
  padding: 8px 0px 8px 8px;
}

#write {
  background: url('<%= asset_path('fbw.gif') %>');
  font-size: 12px;
  margin-bottom: 8px;
  font-weight: bold;
}

#new {
  margin-left: 5px;
  color: #fff;
  padding-left: 5px;
  background: url('<%= asset_path('frot.gif') %>');
}

#more {
  padding-left: 10px;
  font-weight: normal;
}
