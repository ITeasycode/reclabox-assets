}
.pie {
    display: inline-block;
    vertical-align: middle;
    border-radius: 100%;
    fill: #900;
    height: 35px;
    width: 35px;
    position: relative;
}
.pir svg{
    position: absolute;
    top: -6px;
    left: -6px;
    z-index: 5;
}
.emotion-block{
    padding: 30px 0;
}
.emotion-block ul{
    padding:0;
}
.emotion-block ul li{
    list-style:none;
    display: inline-block;
    padding: 0 9px;
    position: relative;
}
.emotion-block ul li:first-child{
    padding-left:0!important;
}
.emotion-block ul li:last-child{
    padding-right:0!important;
}
.emotion-block p{
    text-align: center;
}
.emotion-block .emotion-icon{
    position: absolute;
    top: 6px;
    left: 15px;

}

.emotion-block .emotion-icon.first-icon{
    top: 7px;
    left: 6px;
}
.emotion-block .emotion-icon.fifth-icon{
    top: 7px;
    left: 16px;
}
.emotion-block .cls-7.fill{
    fill:#575756
}
.emotion-block .cls-5.fill{
    fill:#fff;
}
