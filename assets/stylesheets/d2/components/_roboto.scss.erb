@font-face {
    font-family: "Roboto";
    src: local(Roboto Thin), url('<%= asset_path("d2/roboto/Roboto-Thin.eot") %>');
    src: url("<%= asset_path("d2/roboto/Roboto-Thin.eot?#iefix") %>") format('embedded-opentype'),
        url("<%= asset_path("d2/roboto/Roboto-Thin.woff2") %>") format("woff2"),
        url("<%= asset_path("d2/roboto/Roboto-Thin.woff") %>") format("woff"),
        url("<%= asset_path("d2/roboto/Roboto-Thin.ttf") %>") format("truetype");

    font-weight: 200;
}
