.rbi {
  font-size: inherit;
  display: inline-block;
  position: relative;
  line-height: 1;
  height: 15px;
  width: 15px; }
.rbi::before {
  bottom: 0;
  content: "";
  display: inherit;
  height: auto;
  left: 0;
  min-height: 100%;
  min-width: 100%;
  position: relative;
  right: 0;
  top: 0;
  vertical-align: text-bottom;
  z-index: 9; }
.rbi.medium {
  font-size: inherit;
  height: 20px !important;
  width: 20px !important; }
.rbi.large {
  height: 33px !important;
  left: -5px !important;
  top: 3px !important;
  width: 33px !important; }
