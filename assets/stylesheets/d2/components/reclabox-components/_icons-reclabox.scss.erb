@font-face {
	font-family: 'fontello';
  src: url("<%= asset_path("d2/fontello/fontello.eot?72446603")%>");
  src: url("<%= asset_path("d2/fontello/fontello.eot?72446603#iefix")%>") format('embedded-opentype'),
  url("<%= asset_path("d2/fontello/fontello.woff2?72446603")%>") format('woff2'),
  url("<%= asset_path("d2/fontello/fontello.woff?72446603")%>") format('woff'),
  url("<%= asset_path("d2/fontello/fontello.ttf?72446603")%>") format('truetype'),
  url("<%= asset_path("d2/fontello/fontello.svg?72446603#fontello")%>") format('svg');
	font-weight: normal;
	font-style: normal;
}

/* Chrome hack: SVG is rendered more smooth in Windozze. 100% magic, uncomment if you need it. */
/* Note, that will break hinting! In other OS-es font will be not as sharp as it could be */
/*
@media screen and (-webkit-min-device-pixel-ratio:0) {
  @font-face {
    font-family: 'fontello';
    components: url('../font/fontello.svg?72446603#fontello') format('svg');
  }
}
*/
[class^="rbi-"],
[class*=" rbi-"] {
	&:before {
		font: normal normal normal 24px/1 "fontello";
		font-size: inherit;
		speak: none;

		display: inline-block;
		text-decoration: inherit;
		text-rendering: auto;

		/* For safety - reset parent styles, that can break glyph codes*/
		font-variant: normal;
		text-transform: none;

		/* Font smoothing. That was taken from TWBS */
		-webkit-font-smoothing: antialiased;
		-moz-osx-font-smoothing: grayscale;

		/* Uncomment for 3D effect */
		/* text-shadow: 1px 1px 1px rgba(127, 127, 127, 0.3); */
	}
}
