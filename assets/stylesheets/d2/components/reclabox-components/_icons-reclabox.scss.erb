.rbi {
  font-size: inherit;
  display: inline-block;
  position: relative;
  line-height: 1;
  height: 15px;
  width: 15px;

  &::before {
    bottom: 0;
    content: "";
    display: inherit;
    height: auto;
    left: 0;
    min-height: 100%;
    min-width: 100%;
    position: relative;
    right: 0;
    top: 0;
    vertical-align: text-bottom;
    z-index: 9;
  }

  &.medium {
    font-size: inherit;
    height: 20px !important;
    width: 20px !important;
  }

  &.large {
    height: 33px !important;
    left: -5px !important;
    top: 3px !important;
    width: 33px !important;
  }

  &-beschwerde-2 {
    &::before {
      background: url("<%= asset_path("d2/icon-svg/beschwerde-2.svg") %>") no-repeat 0 0;
    }
