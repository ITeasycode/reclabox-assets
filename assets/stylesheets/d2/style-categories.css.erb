@font-face {
  font-family: 'Roboto-Medium';
  font-weight: normal;
  font-style: normal;
  src: url('<%= asset_path("d2/roboto/Roboto-Medium.eot") %>');
  src: url('<%= asset_path("d2/roboto/Roboto-Medium.eot?#iefix") %>') format('embedded-opentype'), url('<%= asset_path("d2/roboto/Roboto-Medium.woff") %>') format('woff'), url('<%= asset_path("d2/roboto/Roboto-Medium.ttf") %>') format('truetype'), url('<%= asset_path("d2/roboto/Roboto-Medium.svg#icon") %>') format('svg');
}
.categories .categories-item .collection {
  border: none;
  display: inline-block;
  margin: 0 0 13px 0;
}
.categories .categories-item .collection .collection-header {
  display: table;
  background: #eceff1;
  border: none;
  -webkit-border-radius: 5px;
          border-radius: 5px;
  padding: 0 5px;
  margin: 0 0 15px 0;
  height: 50px;
}
.categories .categories-item .collection .collection-header h2 {
  display: table-cell;
  vertical-align: middle;
  font: 15px/18px "Roboto-Medium";
  margin: 0;
  padding: 0;
}
.categories .categories-item .collection .collection-header h2 a {
  color: #607d8b;
  -webkit-transition: all 0.6s ease 0s;
  transition: all 0.6s ease 0s;
}
.categories .categories-item .collection .collection-header h2 a:hover {
  color: #99aeb8;
}
.categories .categories-item .collection .collection-header .icon {
  padding: 0 30px 0 42px;
}
.categories .categories-item .collection .collection-header .icon-bicycle {
  background: url("<%= asset_path("d2/icon-svg-cat/bicycle.svg") %>") 9px center / 25px no-repeat;
