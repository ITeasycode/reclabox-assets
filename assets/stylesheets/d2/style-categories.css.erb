@media screen and (min-width: 640px) {
  .categories {
    margin: 0 auto;
    -webkit-column-count: 2;
       -moz-column-count: 2;
            column-count: 2;
  }
}
@media screen and (min-width: 980px) {
  .categories {
    margin: 0 auto;
    -webkit-column-count: 3;
       -moz-column-count: 3;
            column-count: 3;
  }
}
@media screen and (min-width: 1200px) {
  .container {
    min-width: 1170px;
  }
  .categories {
    -webkit-column-count: 4;
       -moz-column-count: 4;
            column-count: 4;
  }
}
