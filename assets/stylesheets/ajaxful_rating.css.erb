ul.ajaxful-rating {
    list-style: none;
    margin: 2px 0;
}

ul.ajaxful-rating li.star {
    float: left;
    width: 14px;
    height: 12px;
}

ul.ajaxful-rating li.clearer { clear: both; }
ul.ajaxful-rating li a { text-decoration: none; }
ul.ajaxful-rating li a img { border: none; }
ul.ajaxful-rating li.on { background: url(<%= asset_path('ajaxful_rating/star_on.gif') %>) top center no-repeat; }
ul.ajaxful-rating li.off { background: url(<%= asset_path('ajaxful_rating/star_off.gif') %>) top center no-repeat; }
ul.rateable li.star:hover { background: url(<%= asset_path('ajaxful_rating/star_hover.gif') %>) top center no-repeat; }
